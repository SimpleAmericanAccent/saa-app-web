{
  "name": "saa-monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "// ============= Setup ============= ": "",
    "migrate": "pnpm --filter backend run migrate",
    "seed:cmu": "pnpm --filter backend run seed:cmu",
    "setup": "pnpm install --frozen-lockfile && pnpm run generate && mkcert localhost && pnpm run setup:env",
    "setup:prod": "pnpm install --frozen-lockfile && pnpm run generate",
    "setup:env": "node scripts/setup-env.js",
    "generate": "pnpm --filter backend run generate",
    "// ============= Admin App ============= ": "",
    "dev:admin": "concurrently \"pnpm --filter admin-api run dev\" \"pnpm --filter admin-web run dev\"",
    "build:admin": "pnpm run setup:prod && pnpm --filter admin-web run build",
    "start:admin": "pnpm --filter admin-api run start",
    "dev:a": "pnpm run dev:admin",
    "build:a": "pnpm run build:admin",
    "start:a": "pnpm run start:admin",
    "studio:a": "pnpm --filter backend run studio:admin",
    "// ============= User App ============= ": "",
    "dev:user": "concurrently \"pnpm --filter user-api run dev\" \"pnpm --filter user-web run dev\"",
    "build:user": "pnpm run setup:prod && pnpm --filter user-web run build",
    "start:user": "pnpm --filter user-api run start",
    "dev": "pnpm run dev:user",
    "dev:u": "pnpm run dev:user",
    "build:u": "pnpm run build:user",
    "start:u": "pnpm run start:user",
    "studio": "pnpm run studio:user",
    "studio:u": "pnpm --filter backend run studio:user",
    "// ============= Shared ============= ": "",
    "lint": "eslint .",
    "test": "pnpm -r test",
    "test:frontend": "pnpm --filter frontend run test",
    "test:f": "pnpm run test:frontend",
    "storybook": "pnpm --filter frontend run storybook",
    "sb": "pnpm run storybook",
    "// ============= Dependency Analysis ============= ": "",
    "depcruise": "dependency-cruiser --config .config/.dependency-cruiser.cjs packages apps",
    "depcruise:graph": "dependency-cruiser --config .config/dependency-cruiser.cjs --output-type dot packages apps | dot -T svg > dependency-graph.svg",
    "depcruise:archi": "dependency-cruiser --config .config/dependency-cruiser.cjs --output-type archi packages apps | dot -T svg > architecture-graph.svg",
    "depcruise:json": "dependency-cruiser --config .config/dependency-cruiser.cjs --output-type json packages apps > dependency-report.json",
    "depcruise:html": "dependency-cruiser --config .config/dependency-cruiser.cjs --output-type html packages apps > dependency-report.html"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.7.6",
    "@mdx-js/react": "^3.1.0",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-progress": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.3.4",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@wavesurfer/react": "^1.0.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "d3": "^7.9.0",
    "d3-sankey": "^0.12.3",
    "express": "^4.19.2",
    "express-openid-connect": "^2.17.1",
    "express-rate-limit": "^7.5.1",
    "lucide-react": "^0.483.0",
    "posthog-js": "^1.255.1",
    "prop-types": "^15.8.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-force-graph-2d": "^1.27.1",
    "react-lite-youtube-embed": "^2.5.1",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^2.1.7",
    "react-router": "^7.3.0",
    "react-router-dom": "^7.4.0",
    "recharts": "^3.1.2",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.14",
    "tailwindcss-animate": "^1.0.7",
    "wavesurfer.js": "^7.11.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.25.0",
    "@mdx-js/rollup": "^3.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.13.10",
    "@types/react": "~19.0.10",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "dependency-cruiser": "^17.2.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^30.2.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
