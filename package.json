{
  "name": "saa-monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "// ============= Setup ============= ": "",
    "seed:cmu": "node shared/scripts/seedCmuAndSubtlex.js",
    "generate": "pnpm --filter core-backend-node run generate",
    "setup": "pnpm install --frozen-lockfile && pnpm run generate",
    "// ============= Admin App ============= ": "",
    "dev:admin": "concurrently \"pnpm --filter admin-backend-node run dev\" \"pnpm --filter admin-frontend-web run dev\"",
    "build:admin": "pnpm run setup && pnpm --filter admin-frontend-web run build",
    "start:admin": "pnpm --filter admin-backend-node run start",
    "dev:a": "pnpm run dev:admin",
    "build:a": "pnpm run build:admin",
    "start:a": "pnpm run start:admin",
    "// ============= User App ============= ": "",
    "dev:user": "concurrently \"pnpm --filter user-backend-node run dev\" \"pnpm --filter user-frontend-web run dev\"",
    "build:user": "pnpm run setup && pnpm --filter user-frontend-web run build",
    "start:user": "pnpm --filter user-backend-node run start",
    "dev:u": "pnpm run dev:user",
    "build:u": "pnpm run build:user",
    "start:u": "pnpm run start:user",
    "// ============= Shared ============= ": "",
    "lint": "eslint ."
  },
  "dependencies": {
    "express": "^4.19.2",
    "express-openid-connect": "^2.17.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@mdx-js/react": "^3.1.0",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-progress": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.3.4",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.1.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "d3": "^7.9.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.483.0",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-force-graph-2d": "^1.27.1",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^2.1.7",
    "react-router": "^7.3.0",
    "react-router-dom": "^7.4.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.0.2",
