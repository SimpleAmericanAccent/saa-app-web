{
  "name": "saa-monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "// Setup": "",
    "seed:cmu": "node shared/scripts/seedCmuAndSubtlex.js",
    "generate": "pnpm --filter core-backend-node exec prisma generate",
    "setup": "pnpm install --frozen-lockfile && pnpm run generate",
    "// Admin App": "",
    "dev:admin": "concurrently \"pnpm --filter admin-backend-node run dev\" \"pnpm --filter admin-frontend-web run dev\"",
    "build:admin": "pnpm run setup && pnpm --filter admin-frontend-web run build",
    "start:admin": "pnpm --filter admin-backend-node run start",
    "dev:a": "pnpm run dev:admin",
    "build:a": "pnpm run build:admin",
    "start:a": "pnpm run start:admin",
    "// User App": "",
    "dev:user": "concurrently \"pnpm --filter user-backend-node run dev\" \"pnpm --filter user-frontend-web run dev\"",
    "build:user": "pnpm run setup && pnpm --filter user-frontend-web run build",
    "start:user": "pnpm --filter user-backend-node run start",
    "dev:u": "pnpm run dev:user",
    "build:u": "pnpm run build:user",
    "start:u": "pnpm run start:user"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/js": "^9.21.0",
    "@mdx-js/rollup": "^3.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.13.10",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@types/react-test-renderer": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.2",
    "prisma": "^6.7.0",
    "react-test-renderer": "18.3.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
