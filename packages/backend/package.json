{
  "name": "backend",
  "version": "1.0.0",
  "description": "SAA backend core",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node --watch server.js",
    "seed:ortho": "node prisma/scripts/seedOrthoWords.js",
    "seed:cmu": "node prisma/scripts/seedCmuAndSubtlex.js",
    "generate": "prisma generate --no-hints",
    "studio": "pnpm run studio:user",
    "studio:user": "node --env-file=.env --env-file=../../apps/user-api/.env node_modules/prisma/build/index.js studio",
    "studio:admin": "node --env-file=.env --env-file=../../apps/admin-api/.env node_modules/prisma/build/index.js studio",
    "migrate": "pnpm run migrate:dev:user",
    "migrate:dev:user": "node --env-file=.env --env-file=../../apps/user-api/.env node_modules/prisma/build/index.js migrate dev",
    "migrate:dev:admin": "node --env-file=.env --env-file=../../apps/admin-api/.env node_modules/prisma/build/index.js migrate dev"
  },
  "author": "Will Rosenberg",
  "license": "UNLICENSED",
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "shared": "workspace:*"
  },
  "devDependencies": {
    "prisma": "^6.7.0"
  }
}
