{
  "name": "backend",
  "version": "1.0.0",
  "description": "SAA backend core",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node --watch server.js",
    "seed:cmu": "node prisma/scripts/seed-cmu-and-subtlex.js",
    "seed:phonemes": "node prisma/scripts/seed-phonemes.js",
    "seed:quiz": "node prisma/scripts/seed-quiz.js",
    "seed:cmu:demo": "node --env-file=.env --env-file=../../apps/user-api/.env.demo prisma/scripts/seed-cmu-and-subtlex.js",
    "seed:phonemes:demo": "node --env-file=.env --env-file=../../apps/user-api/.env.demo prisma/scripts/seed-phonemes.js",
    "seed:quiz:demo": "node --env-file=.env --env-file=../../apps/user-api/.env.demo prisma/scripts/seed-quiz.js",
    "export:quiz": "node prisma/scripts/export-quiz-data.js",
    "seed:quiz:local": "node --env-file=.env --env-file=../../apps/user-api/.env prisma/scripts/seed-quiz.js",
    "export:quiz:local": "node --env-file=.env --env-file=../../apps/user-api/.env prisma/scripts/export-quiz-data.js",
    "generate": "prisma generate --no-hints",
    "studio": "pnpm run studio:user",
    "studio:user": "node --env-file=.env --env-file=../../apps/user-api/.env node_modules/prisma/build/index.js studio",
    "studio:user:demo": "node --env-file=.env --env-file=../../apps/user-api/.env.demo node_modules/prisma/build/index.js studio",
    "studio:admin": "node --env-file=.env --env-file=../../apps/admin-api/.env node_modules/prisma/build/index.js studio",
    "migrate": "pnpm run migrate:dev:user",
    "migrate:dev:user": "node --env-file=.env --env-file=../../apps/user-api/.env node_modules/prisma/build/index.js migrate dev",
    "migrate:dev:admin": "node --env-file=.env --env-file=../../apps/admin-api/.env node_modules/prisma/build/index.js migrate dev"
  },
  "author": "Will Rosenberg",
  "license": "UNLICENSED",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.946.0",
    "@aws-sdk/s3-request-presigner": "^3.946.0",
    "@prisma/client": "^6.7.0",
    "cors": "^2.8.5",
    "shared": "workspace:*"
  },
  "devDependencies": {
    "prisma": "^6.7.0"
  }
}
